---
import BaseLayout from "../layouts/BaseLayout.astro";

const s = (o: any) => JSON.stringify(o, null, 2);
---

<BaseLayout title="Test page" description="The page for testing">
  <pre>{s(Astro)}</pre>
  <pre>
        URL: {s(Astro.url)}
        Request: {s(Astro)}
        Site: {Astro.site}
        Image: {new URL("favicon-512.png", Astro.url)}
    </pre>
</BaseLayout>
